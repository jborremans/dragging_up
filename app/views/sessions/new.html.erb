<div id="signincontainer">
<!--<h1>Log In</h1>-->

<div class="formcontainer">
	<div class="formheader">
	<h5>Existing Customers</h5>
	<h1>Sign in here</h1>
	</div>
<%= form_tag "/sessions/create" do %>

<p>
  <%= label_tag :email %>
  <%= text_field_tag :email %>
</p>
<p>
    <%= label_tag :password %>
    <%= password_field_tag :password %>
</p>
    <div class="submitbox">
  <%= submit_tag "Please let me in!" %>
  </div>
<% end %>
</div>
<ul>
<%= link_to 'Back to Home', home_url %>
</ul>
</div>